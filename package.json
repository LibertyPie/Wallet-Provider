{
  "name": "@libertypie/wallet-provider",
  "main": "./lib/src/index.js",
  "version": "1.0.3",
  "description": "A cross-blockchain single api to connect between dApps & wallets",
  "scripts": {
    "watcher-dev": "node ./tools/watcher.js --watch_dir --path ./src/assets/",
    "build-dev": "npx tsc --watch --project tsconfig.json",
    "dev": "npx concurrently \"npm:watcher-dev\" \"npm:build-dev\"",
    "watcher": "node ./tools/watcher.js  --path ./src/assets/",
    "build": "npm run watcher && npx tsc --project tsconfig.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "Wallet provider",
    "Web3",
    "ethersjs",
    "ethereum",
    "binance",
    "smart",
    "chain",
    "Trust Wallet",
    "metamask",
    "binance chain",
    "coinbase wallet",
    "sync wallet",
    "Dapp",
    "Wallet"
  ],
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/libertypie/wallet-Provider.git"
  },
  "homepage": "https://wallet-provider.libertypie.com",
  "author": {
    "name": "LibertyPie",
    "email": "hello@libertypie.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/node": "^14.14.16",
    "arg": "^5.0.0",
    "colors": "^1.4.0",
    "concurrently": "^5.3.0",
    "copy-and-watch": "^0.1.5",
    "glob": "^7.1.6",
    "jss-cli": "^6.0.2",
    "node-sass": "^5.0.0",
    "node-watch": "^0.7.1",
    "patch-package": "^6.2.2",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.13",
    "typescript": "^4.1.3",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1"
  },
  "dependencies": {}
